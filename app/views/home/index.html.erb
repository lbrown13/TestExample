<div id="request-invite-a" class="modal hide fade in" style="display:">
  <%= simple_form_for resource, :as => resource_name, :url => registration_path(resource_name) , :html => {:class => 'form-horizontal' } do |f| %>
  <div class="modal-header">
    <a class="close" data-dismiss="modal">&#215;</a>
    <h3>Sign Up for our Free Weekly Newsletter</h3>
  </div>
  <div class="modal-body">
    <%= f.error_notification %>
    <%= f.input :name, :placeholder => 'Enter Your Name' %>
    <%= f.input :email, :placeholder => 'user@example.com' %>
  </div>
  <div class="modal-footer">
    <%= f.submit "Let's Do It!", :class => "btn btn-primary", :id => 'invitation_button' %>
    <a class="btn" data-dismiss="modal" href="">Close</a>
  </div>
  <% end %>
</div>

